#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

typedef struct {
    char chave;
}item;

typedef struct NoArvore *Ponteiro;

typedef struct nozin{
    item elemento;
    Ponteiro esquerda;
    Ponteiro direita;
}NoArvore;

bool iniciar(Ponteiro *tree){
    (*tree)=NULL;
}
void ArqEntrada(){
    FILE *arq=fopen("entrada.txt","r");
    char c[100];
    int n=0;

    if(arq != NULL){
        while((c=fgets(arq,'sizeof c, stdin)) != EOF){
            if(c =='cafe' || c == 'batata' )n++;
        }
    }

    fclose(arq);
    printf("existe %d As\n",n);
}

int main(){

    ArqEntrada();
    return 0;
}
